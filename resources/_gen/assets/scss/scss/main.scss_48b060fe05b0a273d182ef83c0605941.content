.cursor{animation:blink .8s step-start infinite}@keyframes blink{50%{opacity:0}}html{font-size:21px;font-family:lucida console,Monaco,monospace;background-color:#343434;color:#dfdfdf}a{text-decoration:underline dotted #f2b880}a:visited{color:#dfdfdf;text-decoration:underline dotted #f2b880}nav{width:100%;display:block}nav #nav-contain{width:90%;margin:20px auto 0}nav #nav-contain input{display:none}nav #nav-contain #nav-logo{display:inline-block}nav #nav-contain #nav-logo span{font-weight:700;color:#f2b880}nav #nav-contain #nav-logo #nav-logo-path{margin-top:5px}nav #nav-contain #nav-logo #nav-logo-path #nav-logo-cursor{background-color:#dfdfdf;display:inline-block;position:relative;height:24px;width:12px;top:5px}nav #nav-contain #nav-dropdown{float:right;top:5px;width:35px;height:38px;position:relative;text-align:center;display:inline-block;border:1px dotted #f2b880;border-radius:1px;font-size:30px}nav #nav-contain #nav-dropdown label{width:100%;height:100%;display:block;cursor:pointer}nav #nav-contain #nav-dropdown:hover{background-color:#9a7aa0}nav #nav-contain #nav-links{align-self:center;display:none;clear:both;list-style-type:none;width:100%}nav #nav-contain #nav-toggle:checked~#nav-links{display:inline-block}