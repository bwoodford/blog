.cursor{animation:blink .8s step-start infinite}@keyframes blink{50%{opacity:0}}html{font-size:21px;font-family:lucida console,Monaco,monospace;background-color:#343434;color:#dfdfdf}a{text-decoration:underline dotted #f2b880}a:visited{color:#dfdfdf;text-decoration:underline dotted #f2b880}nav{width:100%;display:flex}nav #nav-contain{display:flex;width:100%;flex-flow:row wrap;justify-content:space-between;padding:20px}nav #nav-contain #nav-logo span{font-weight:700;color:#f2b880}nav #nav-contain #nav-logo #nav-logo-path{margin-top:5px}nav #nav-contain #nav-logo #nav-logo-path #nav-logo-cursor{background-color:#dfdfdf;display:inline-block;position:relative;height:24px;width:12px;top:5px}nav #nav-contain #nav-dropdown{border:1px dotted #f2b880;position:relative;max-height:40px;border-radius:1px;padding:1px 10px 0;font-size:30px;cursor:pointer}nav #nav-contain #nav-dropdown:hover{background-color:#9a7aa0}nav #nav-contain #nav-links{display:none}